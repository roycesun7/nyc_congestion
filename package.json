{
  "name": "congestion-dashboard",
  "version": "1.0.0",
  "description": "NYC Congestion Dashboard",
  "scripts": {
    "dev": "npm run tailwind:watch",
    "tailwind:watch": "tailwindcss -i ./static/css/input.css -o ./static/css/style.css --watch",
    "tailwind:build": "tailwindcss -i ./static/css/input.css -o ./static/css/style.css --minify"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.303.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  }
}